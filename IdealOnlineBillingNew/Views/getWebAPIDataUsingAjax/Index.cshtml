        <script>

            // Initialize the DataTable
            $(document).ready(function () {
                loadData();
            });

            function loadData() {
                alert('Ok');
                $.ajax(
                    {
                        url: "http://localhost:51962/api/Employee/Index",
                        type: "GET",
                        contentType: "application/json;charset=utf-8",
                        dataType: "json",
                        crossDomain: true,
                        success: function (result) {
                           
                            console.log(result);
                            alert(result);
                        }
                    });
            }
        </script>
