
@{
    ViewBag.Title = "Index";
    
}
<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2>DASHBOARD</h2>
        </div>
        <h2>Index</h2>
        
        <input  type="submit" value="Submit" onclick="loadData()"/>
